<% num = @dates.size %>
<div class="row">
  <div class="span12">
    <ul class="breadcrumb">
      <%= render 'result_header' %>
      <li>
        <%= link_to('Recently', result_recently_path) %> <span class="divider">&gt;</span>
      </li>
      <li>
        <%= num %>weeks <span class="divider">&gt;</span>
      </li>
      <li class="active"><strong>Callers Ranking</strong></li>
    </ul>
  </div>
</div>
<div class="row">
  <div class="span9">
    <h3>Callers Ranking : Recently <%= num %>weeks <%= result_subtitle %></h3>
  </div>
</div>
<% unless gvtype_te? %>
  <div class="row">
    <div class="offset6 span3">
      priority : <%= link_to_unless(result_view_for_all?,  'Total', result_recently_rank_path(num: num)) %>
      / <%= link_to_unless(result_view_for_se?, 'SE', result_recently_rank_path(num: num, priority: 'se')) %>
      / <%= link_to_unless(result_view_for_fe?, 'FE', result_recently_rank_path(num: num, priority: 'fe')) %>
    </div>
  </div>
<% end %>
<div class="row">
  <div class="span9">
    <hr/>
  </div>
</div>
<%= render 'caller_ranking', callers: @callers, dates: @dates, target: :recently %>
